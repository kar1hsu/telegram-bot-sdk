{
  "name": "karlhsu/telegram-bot-sdk",
  "type": "library",
  "description": "The Telegram Bot API PHP SDK",
  "keywords": [
    "telegram",
    "chatbot",
    "telegram php",
    "telegram bot",
    "telegram chatbot",
    "telegram bot api",
    "telegram bot sdk",
    "telegram-bot-sdk",
    "telegram bot php sdk",
    "telegram bot api php sdk",
    "php telegram bot",
    "laravel telegram"
  ],
  "homepage": "https://github.com/telegram-bot-sdk/telegram-bot-sdk",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Irfaq Syed",
      "email": "syed+gh@lukonet.com",
      "homepage": "https://github.com/irazasyed"
    },
    {
      "name": "Telegram Bot SDK Community",
      "homepage": "https://github.com/telegram-bot-sdk/telegram-bot-sdk/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.0 || ^8.0",
    "illuminate/support": "^6.0 || ^7.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpspec/prophecy": "^1.5",
    "phpunit/phpunit": "^8.5 || ^9.0"
  },
  "suggest": {
    "illuminate/container": "Hold dependencies to be injected in commands constructors",
    "irazasyed/larasupport": "Allows you to use any Laravel Package in Lumen by adding support!"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Telegram\\Bot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Telegram\\Bot\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "format": "php-cs-fixer fix",
    "test": "phpunit"
  }
}
